<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.fszy.widgetdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TextViewAndEditTextDemo"/>
        <activity android:name=".ButtonDemo"/>
        <activity android:name=".RadioButtonAndCheckBoxDemo"/>
        <activity android:name=".ImageViewDemo"/>
        <activity android:name=".SpinnerDemo"/>
    </application>

</manifest>